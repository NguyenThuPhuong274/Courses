<script>
    $('.cat-item a').click(function(){
        var check = false;
    if($(this).parent().hasClass('opened')) {
        check = true;
    }

    $(this).parent().parent().find('li.opened').removeClass('opened');
    if (check) {
        $(this).parent().addClass('opened');
    }

    $(this).parent().toggleClass('opened');
})

</script>

